          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="deliveryOptions" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="deliveryOption" type="DeliveryOption" minOccurs="0" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>
                  TODO: add documentation
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
