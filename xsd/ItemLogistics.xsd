  <xsd:simpleType name="FulfillmentType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ELECTRONIC"/>
      <xsd:enumeration value="PICKUP"/>
      <xsd:enumeration value="DELIVERY"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="FulfillmentOption">
    <xsd:sequence>
