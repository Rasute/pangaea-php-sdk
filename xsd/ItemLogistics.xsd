      <xsd:element name="availabilityThreshold" type="AvailabilityThreshold">
        <xsd:annotation>
          <xsd:documentation>            
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="GiftOptions">
    <xsd:sequence>
      <xsd:element name="shipNodeOrgId" type="xsd:string" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>            
          UUID
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="legacyDistributorId" type="xsd:string" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="allowGiftWrap" type="xsd:boolean" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>          
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="allowGiftMessage" type="xsd:boolean" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>            
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="allowGiftReceipt" type="xsd:boolean" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="PreferredDistributor">
    <xsd:sequence>
      <xsd:element name="itemShipNodeOrgId" type="xsd:string" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>            
          UUID
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="legacyDistributorId" type="xsd:string" minOccurs="0" maxOccurs="1">
